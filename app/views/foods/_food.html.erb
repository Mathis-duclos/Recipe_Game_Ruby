<div id="<%= dom_id food %>">
  <p>
    <strong>Name:</strong>
    <%= food.name %>
  </p>

  <p>
    <strong>Game:</strong>
    <%= food.game_id %>
  </p>

  <p>
    <strong>Image:</strong>
    <%= food.image %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= food.description %>
  </p>

  <p>
    <strong>Recipe:</strong>
    <%= food.recipe_id %>
  </p>

</div>
